{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "coverage": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "typescript": {
      "outputs": []
    },
    "extract-translations": {
      "outputs": []
    }
  }
}
