{
    "name": "@sihsalus/esm-dyaku-integration",
    "version": "1.0.0",
    "description": "Integración con DYAKU FHIR y Padrón Nacional para SIH SALUS",
    "browser": "dist/sihsalus-esm-dyaku-integration.js",
    "main": "src/index.ts",
    "source": true,
    "license": "MPL-2.0",
    "scripts": {
        "start": "openmrs develop",
        "serve": "webpack serve --mode=development",
        "debug": "npm run serve",
        "build": "webpack --mode production",
        "analyze": "webpack --mode=production --env analyze=true",
        "lint": "eslint src --ext ts,tsx",
        "lint:fix": "eslint src --ext ts,tsx --fix",
        "prettier": "prettier --write \"src/**/*.{ts,tsx}\" --list-different",
        "typescript": "tsc --noEmit",
        "extract-translations": "i18next 'src/**/*.{ts,tsx}' --config ../../tools/i18next-parser.config.js",
        "test": "jest --config jest.config.js --passWithNoTests",
        "test:watch": "jest --config jest.config.js --watch --passWithNoTests",
        "coverage": "jest --config jest.config.js --coverage",
        "verify": "turbo lint typescript test",
        "clean": "rimraf dist"
    },
    "browserslist": [
        "extends browserslist-config-openmrs"
    ],
    "keywords": [
        "openmrs",
        "microfrontends",
        "dyaku",
        "fhir",
        "padron-nacional",
        "peru",
        "sih-salus",
        "integration"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tu-org/sihsalus-esm-modules.git"
    },
    "homepage": "https://github.com/tu-org/sihsalus-esm-modules#readme",
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/tu-org/sihsalus-esm-modules/issues"
    },
    "author": "SIH SALUS Team",
    "files": [
        "dist",
        "src",
        "translations",
        "package.json",
        "README.md"
    ],
    "dependencies": {
        "@sihsalus/esm-sihsalus-library": "workspace:*",
        "@carbon/react": "^1.76.0",
        "classnames": "^2.5.1",
        "lodash-es": "^4.17.21"
    },
    "peerDependencies": {
        "@openmrs/esm-framework": "next",
        "dayjs": "1.x",
        "react": "18.x",
        "react-i18next": "15.x",
        "react-router-dom": "6.x"
    },
    "devDependencies": {
        "@openmrs/esm-framework": "next",
        "@swc/cli": "^0.3.12",
        "@swc/core": "^1.7.0",
        "@swc/jest": "^0.2.36",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "^29.5.13",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.1",
        "@types/webpack-env": "^1.18.5",
        "@typescript-eslint/eslint-plugin": "^8.8.1",
        "@typescript-eslint/parser": "^8.8.1",
        "css-loader": "^7.1.2",
        "dayjs": "^1.11.13",
        "eslint": "^8.57.1",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "openmrs": "next",
        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-i18next": "^15.0.2",
        "react-router-dom": "^6.26.2",
        "sass": "^1.79.4",
        "sass-loader": "^16.0.2",
        "swc-loader": "^0.2.6",
        "typescript": "^5.6.3",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "packageManager": "yarn@4.5.1"
}