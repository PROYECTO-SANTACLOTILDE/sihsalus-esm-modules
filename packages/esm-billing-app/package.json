{
  "name": "@pucp-gidis-hiisc/esm-billing-app",
  "version": "1.0.4",
  "description": "Módulo de facturación para SIH SALUS - Sistema Integral Hospitalario",
  "browser": "dist/pucp-gidis-hiisc-esm-billing-app.js",
  "main": "src/index.ts",
  "license": "MPL-2.0",
  "scripts": {
    "start": "openmrs develop",
    "serve": "webpack serve --mode=development",
    "debug": "npm run serve",
    "build": "webpack --mode production",
    "analyze": "webpack --mode=production --env analyze=true",
    "lint": "cross-env eslint src --ext ts,tsx",
    "test": "cross-env TZ=UTC jest --config jest.config.js --verbose false --passWithNoTests --color",
    "test:watch": "cross-env TZ=UTC jest --watch --config jest.config.js --color",
    "coverage": "yarn test --coverage",
    "typescript": "tsc",
    "extract-translations": "i18next 'src/**/*.component.tsx' 'src/**/*.extension.tsx' 'src/**/*modal.tsx' 'src/**/*.workspace.tsx' 'src/index.ts' --config ../../tools/i18next-parser.config.js"
  },
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "keywords": [
    "openmrs",
    "sihsalus"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/PROYECTO-SANTACLOTILDE/sihsalus-esm-modules#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PROYECTO-SANTACLOTILDE/sihsalus-esm-modules.git"
  },
  "bugs": {
    "url": "https://github.com/PROYECTO-SANTACLOTILDE/sihsalus-esm-modules/issues"
  },
  "dependencies": {
    "@carbon/react": "^1.83.0",
    "@hookform/resolvers": "^3.3.4",
    "classnames": "^2.5.1",
    "fuzzy": "^0.1.3",
    "lodash-es": "^4.17.21",
    "react-hook-form": "^7.49.3",
    "react-to-print": "^2.14.15",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@openmrs/esm-framework": "6.x",
    "@openmrs/esm-patient-common-lib": "10.x",
    "dayjs": "1.x",
    "react": "18.x",
    "react-i18next": "11.x",
    "react-router-dom": "6.x",
    "rxjs": "6.x",
    "swr": "2.x"
  },
  "devDependencies": {
    "webpack": "^5.99.9"
  }
}
